<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="extended.cs.sdsu.edu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="extended.cs.sdsu.edu.ProfessorListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="extended.cs.sdsu.edu.SelectedProfessorDetailsActivity"
            android:label="SelectedProfessorDetails" >
            <intent-filter>
                <action android:name="cs.assignment.intent.action.VIEW_PROFESSOR_DETAILS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="extended.cs.sdsu.edu.RateProfessorActivity"
            android:label="RateSelectedProfessor" >
            <intent-filter>
                <action android:name="cs.assignment.intent.action.RATE_PROFESSOR" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="extended.cs.sdsu.edu.SuccessPage"
            android:label="SuccessPage" >
            <intent-filter>
                <action android:name="cs.assignment.intent.action.SUCCESSFUL" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>